{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": false,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "types": [
      "node",
      "chrome"
    ],
    "paths": {
      "@mui/styled-engine": [
        "./node_modules/@mui/styled-engine-sc"
      ]
    }
  },
  "include": ["./src/**/*"]
}
